<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">

    <!-- 快递公司 -->
    <bean id="expressUtil" class="com.aixuexi.vampire.util.ExpressUtil">
        <property name="syncToWms" value="${syncToWms}"/>
        <property name="splitTips" value="${splitTips}"/>
        <property name="aging" value="${aging}"/>
        <property name="diyTips" value="${diyTips}"/>
        <property name="deliveryTime" value="${deliveryTime}"/>
        <property name="preSaleDeliveryTime" value="${preSaleDeliveryTime}"/>
    </bean>

</beans>